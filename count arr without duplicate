import java.util.*;
class abc1
{
public static void main(String args[])
{
Scanner obj=new Scanner(System.in);
int num= obj.nextInt();
int count=0;
int arr[]=new int[num];
for(int i=0;i<num;i++)
{
arr[i]=obj.nextInt();
}
int val=arr[0];
for(int i=0;i<num;i++)
{
for(int j=i+1;j<num;j++)
{
if(arr[i]==arr[j])
{
count++;
}
}
}
System.out.print(num-count);
}
}


