import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
		Scanner obj=new Scanner(System.in);
		String s=obj.nextLine();
		//char ch[]=s.toCharArray();
		int count=0;
		for(int i=0;i<s.length();i++)
		{
		    if(s.charAt(i)!='\0')
		    count++;
		}
		System.out.println(count);
	}
}
